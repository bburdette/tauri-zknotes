{
  "productName": "zknotes",
  "version": "0.1.0",
  "identifier": "com.zknotes.wtf",
  "build": {
    "frontendDist": "static",
    "devUrl": "http://0.0.0.0:8080"
  },
  "app": {
    "windows": [
      {
        "title": "zknotes",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": {
          "default-src": "'self'",
          "connect-src": "'self'",
          "font-src": ["'self'"],
          "img-src": "'self'",
          "media-src": "'self'",
          "style-src": "'self'"
        }
    }
   },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
